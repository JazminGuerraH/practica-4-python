#ejercicio 5 

meses = ("inicio", "enero", "febrero", "marzo", "abril", "mayo", "junio", "julio", "agosto", "septiembre", "octubre", "noviembre", "diciembre")
salida = False
while not salida:
    numero= int(input("digita un número: "))
    if numero == 0:
        break
    else:
        print(meses[numero])


