#ejercicio 1 
from random import randint
from statistics import median
print("ejercicio 1")
lista1 = []
for i in range (10):
    lista1.append(randint(1,10))
print(lista1)
for i in lista1:
    print(i, i**2, i**3)
print()
print("ejercicio 2")

#ejercicio 2
lista2 = []
for i in range (5):
    palabra = input("agregue una palabra: ")
    lista2.append(palabra)
print(lista2)
lista2.reverse()
print(lista2)
print()
print("ejercicio 3")
#ejercicio 3

lista3 = []
for i in range(5):
    nota = float(input("escribe calificacion: "))
    while(nota<0 or nota>10):
        print("pon otra calificacion: ")
        nota = float(input("escribe calificacion: "))
    lista3.append(nota)
print(lista3)
print(f"la media es: {median(lista3)}, la maxima es: {max(lista3)}, la minima es: {min(lista3)} ")
print()
print("ejercicio 4")

